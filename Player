package newtictactoe;

public class Player {
private String name;
private String symbol;


public Player(String n,String s) {
name=n;
symbol=s;
}

public String getSymbol() {
	return symbol;
}

public void setSymbol(String symbol) {
	this.symbol = symbol;
}

public String getName() {
	return name;
}

public void setName(String name) {
	this.name = name;
} 

public void printGamewinner() {
System.out.print("the winner of the game is:"+getName());

}
}
